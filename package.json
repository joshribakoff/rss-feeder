{
  "name": "rss-clustering",
  "version": "1.0.0",
  "description": "RSS feed article clustering demo",
  "main": "dist/index.js",
  "scripts": {
    "dev:client": "vite",
    "dev:server": "ts-node src/serve.ts",
    "build:client": "vite build",
    "build:server": "rm -fr dist && tsc && rsync -avr prisma/ dist/prisma/ && cd dist && npm ci",
    "build:prod": "npm run build:client && npm run build:server && rsync -avr public/ dist/public/",
    "build": "npm run build:prod",
    "serve:dev": "ts-node src/serve.ts",
    "serve:prod": "node dist/serve.js",
    "start": "ts-node src/index.ts",
    "test": "vitest "
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.13.2",
    "express": "^5.1.0",
    "natural": "^6.12.0",
    "pm2": "^6.0.13",
    "prisma": "^5.6.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@types/natural": "^5.1.5",
    "@types/node": "^20.9.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  }
}
